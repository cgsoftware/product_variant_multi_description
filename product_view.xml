<?xml version="1.0" ?>
<openerp>
  <data>

       <record model="ir.ui.view" id="view_desvariant_type">
            <field name="name">view.desvariant.type</field>
            <field name="model">product.variant.dimension.type</field>
            <field name="inherit_id" ref="product_variant_multi.variant_type_form_view"/>
            <field name="arch" type="xml">
                <field name="name" position="after">
                    <newline/>                  
                    <field name="desc_type"/>	

                </field>
            </field>
        </record>
    

      <record model="ir.ui.view" id="view_desvariant_value">
            <field name="name">view.desvariant.value</field>
            <field name="model">product.variant.dimension.value</field>
            <field name="inherit_id" ref="product_variant_multi.variant_value_form_view"/>
            <field name="arch" type="xml">
                <field name="name" position="after">
                    <newline/>                  
                    <field name="desc_value"/>	

                </field>
            </field>
        </record>
    


  </data>
</openerp>
